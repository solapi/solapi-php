    "korea voice message",
    "korea fax"
  ],
  "authors": [
    {
      "name": "Nurigo CX Team",
      "email": "contact@nurigo.net"
    },
    {
      "name": "Subin Lee",
      "email": "chandar.sblee@gmail.com",
      "homepage": "https://subux.dev"
    }
  ],
  "minimum-stability": "stable",
  "homepage": "https://solapi.com",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  }
